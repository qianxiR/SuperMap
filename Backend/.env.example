# 服务器配置
NODE_ENV=development
PORT=3001
API_PREFIX=/api/v1

# 数据库配置 - PostGIS
DATABASE_URL="postgresql://username:password@localhost:5432/city_dashboard?schema=public"
DB_HOST=localhost
DB_PORT=5432
DB_NAME=city_dashboard
DB_USER=username
DB_PASSWORD=password

# SuperMap服务配置
SUPERMAP_SERVER_URL=http://localhost:8090
SUPERMAP_USERNAME=your_username
SUPERMAP_PASSWORD=your_password
SUPERMAP_MAP_SERVICE=iserver/services/map-WuHan/rest/maps/武汉_市级
SUPERMAP_DATA_SERVICE=iserver/services/data-WuHan/rest/data

# 安全配置
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d
BCRYPT_ROUNDS=12

# CORS配置
CORS_ORIGIN=http://localhost:5173

# 日志配置
LOG_LEVEL=info
LOG_DIR=logs

# 缓存配置
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# API限制
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100