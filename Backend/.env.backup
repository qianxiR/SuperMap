# SuperMap Backend Environment Variables

# Application Settings
APP_NAME=SuperMap GIS + AI Backend
APP_VERSION=1.0.0
DEBUG=true
ENVIRONMENT=development

# API Configuration
API_V1_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:5173

# Security Settings  
SECRET_KEY=your-super-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database Configuration (for future use)
POSTGRES_USER=supermap_user
POSTGRES_PASSWORD=supermap_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=supermap_gis

# Redis Configuration (for future use)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# SuperMap Configuration
SUPERMAP_BASE_URL=http://localhost:8090
SUPERMAP_USERNAME=admin
SUPERMAP_PASSWORD=admin

# OpenAI Configuration (for future use)
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-ada-002

# Logging
LOG_LEVEL=INFO