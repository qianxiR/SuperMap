                      "enableProgress": {
                        "type": "boolean",
                        "description": "是否启用进度显示",
                        "default": true
                      },
                      "returnGeometry": {
                        "type": "boolean",
                        "description": "是否返回几何数据",
                        "default": true
                      }
                    }
                  },
                  "options": {
                    "type": "object",
                    "nullable": true,
                    "properties": {
                      "resultLayerName": {
                        "type": "string",
                        "description": "结果图层名称",
                        "default": "擦除分析结果"
                      },
                      "enableStatistics": {
                        "type": "boolean",
                        "description": "是否启用统计信息",
                        "default": true
                      },
                      "coordinateSystem": {
                        "type": "string",
                        "description": "坐标系统",
                        "default": "EPSG:4326"
                      }
                    }
                  }
                },
                "required": [
                  "targetData",
                  "eraseData"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "擦除分析执行成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnalysisResponse"
                }
              }
            }
          },
          "400": {
            "description": "请求参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
