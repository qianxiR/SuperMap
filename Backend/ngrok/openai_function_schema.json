                      },
                      "features": {
                        "type": "array",
                        "items": {
                          "type": "object"
                        }
                      }
                    },
                    "required": ["type", "features"]
                  },
                  "analysisOptions": {
                    "type": "object",
                    "nullable": true,
                    "properties": {
                      "batchSize": {
                        "type": "number",
