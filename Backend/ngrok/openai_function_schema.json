    "/api/v1/spatial-analysis/shortest-path": {
      "post": {
        "description": "执行最短路径分析",
        "operationId": "ExecuteShortestPathAnalysis",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "startPoint": {
                    "type": "object",
                    "description": "起点坐标",
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": ["Point"]
                      },
                      "coordinates": {
                        "type": "array",
                        "items": {
                          "type": "number"
                        },
                        "minItems": 2,
                        "maxItems": 3
                      }
                    },
                    "required": ["type", "coordinates"]
                  },
                  "endPoint": {
                    "type": "object",
                    "description": "终点坐标",
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": ["Point"]
                      },
                      "coordinates": {
                        "type": "array",
                        "items": {
                          "type": "number"
                        },
                        "minItems": 2,
                        "maxItems": 3
                      }
                    },
                    "required": ["type", "coordinates"]
                  },
                  "obstacleData": {
                    "type": "object",
                    "nullable": true,
                    "description": "障碍物数据GeoJSON格式",
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": ["FeatureCollection"]
                      },
                      "features": {
                        "type": "array",
                        "items": {
                          "type": "object"
                        }
                      }
                    }
                  },
                  "analysisOptions": {
                    "type": "object",
                    "nullable": true,
                    "properties": {
                      "units": {
                        "type": "string",
                        "enum": [
                          "kilometers",
                          "miles",
                          "meters",
                          "degrees",
                          "radians"
                        ],
                        "description": "距离单位",
                        "default": "kilometers"
                      },
                      "resolution": {
                        "type": "number",
                        "minimum": 100,
                        "maximum": 10000,
                        "default": 1000,
                        "description": "分析分辨率"
                      }
                    }
                  },
                  "options": {
                    "type": "object",
                    "nullable": true,
                    "properties": {
                      "returnGeometry": {
                        "type": "boolean",
                        "description": "是否返回几何信息",
                        "default": true
                      },
                      "calculateDistance": {
                        "type": "boolean",
                        "description": "是否计算距离",
                        "default": true
                      },
                      "calculateDuration": {
                        "type": "boolean",
                        "description": "是否计算时长",
                        "default": true
                      },
                      "averageSpeed": {
                        "type": "number",
                        "description": "平均速度（km/h）",
                        "minimum": 1,
                        "maximum": 200,
                        "default": 50
                      }
                    }
                  }
                },
                "required": [
                  "startPoint",
                  "endPoint"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "最短路径分析执行成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnalysisResponse"
                }
              }
            }
          },
          "400": {
            "description": "请求参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
