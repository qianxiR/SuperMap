							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"input\": {\n    \"type\": \"Feature\",\n    \"geometry\": {\n      \"type\": \"Point\",\n      \"coordinates\": [125.3245, 43.8868]\n    },\n    \"properties\": {\n      \"name\": \"长春市中心\",\n      \"type\": \"city_center\"\n    }\n  },\n  \"distance_m\": 500.0,\n  \"cap_style\": \"round\",\n  \"dissolve\": true\n}"
